{
  "preprocessing": [
    {
      "type": "image_resize",
      "width": 84,
      "height": 84
    },
    {
      "type": "grayscale"
    },
    {
      "type": "divide",
      "scale": 255
    },
    {
      "type": "sequence",
      "length": 4
    }
  ],
  "exploration": {
    "type": "epsilon_anneal",
    "epsilon": 1.0,
    "epsilon_final": 0.1,
    "epsilon_timesteps": 1e6
  },
  "reward_preprocessing": [
    {
      "type": "clip",
      "min": -1,
      "max": 1
    }
  ],

  "batch_size": 6,
  "repeat_update": 1,
  "keep_last": true,
  "target_update_frequency": 10000,

  "discount": 0.99,
  "learning_rate": 0.004,
  "optimizer": {
    "type": "rmsprop",
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "tf_summary": null,
  "log_level": "info",
  "update_target_weight": 1.0,
  "double_dqn": false,
  "clip_loss": 1.0
}
