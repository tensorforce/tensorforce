{
  "log_level": "info",
  "memory_capacity": 1e2,
  "batch_size": 32,
  "update_rate": 0.25,
  "update_repeat": 1,
  "use_target_network": true,
  "target_update_frequency": 100,
  "first_update": 5e4,
  "learning_rate": 0.00025,
  "optimizer": {
    "type": "rmsprop",
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "memory": {
      "type": "replay",
      "random_sampling": true
  },
  "double_dqn": false,
  "exploration": {
    "type": "epsilon_decay",
    "epsilon": 1,
    "epsilon_final": 0.1,
    "epsilon_states": 1e6
  },
  "preprocessing": [
    {
      "type": "image_resize",
      "width": 84,
      "height": 84
    },
    {
      "type": "grayscale"
    },
    {
      "type": "center"
    },
    {
      "type": "sequence",
      "length": 4
    }
  ],
  "reward_preprocessing": [
    {
      "type": "clip",
      "min": -1,
      "max": 1
    }
  ],
  "repeat_actions": 1,

  "network_layers": [
    {
      "type": "conv2d",
      "size": 16,
      "window": 8,
      "stride": 4
    },
    {
      "type": "conv2d",
      "size": 8,
      "window": 4,
      "stride": 2
    },
    {
      "type": "flatten"
    },
    {
      "type": "dense",
      "size": 16
    }
  ]

}
