{
    "type": "vpg_agent",

    "update_mode": {
        "unit": "episodes",
        "batch_size": 20,
        "frequency": 20
    },
    "memory": {
        "type": "latest",
        "capacity": 4000,
        "include_next_states": false
    },

    "optimizer": {
        "type": "adam",
        "learning_rate": 1e-2
    },

    "discount": 0.99,

    "entropy_regularization": null,
    "gae_lambda": null,

    "baseline_mode": "states",
    "baseline": {
        "type": "mlp",
        "sizes": [
            32,
            32
        ]
    },
    "baseline_optimizer": {
        "type": "multi_step",
        "optimizer": {
            "type": "adam",
            "learning_rate": 1e-3
        },
        "num_steps": 5
    }
}